{
  "name": "closeclaw",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=apps/web",
    "dev:api": "npm run dev --workspace=apps/api",
    "dev:all": "npm run dev --workspace=apps/web & npm run dev --workspace=apps/api",
    "build": "npm run build --workspaces",
    "lint": "npm run lint --workspaces"
  }
}