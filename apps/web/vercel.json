{
  "rewrites": [
    {
      "source": "/openclaw-hosting",
      "destination": "/openclaw-hosting/index.html"
    },
    {
      "source": "/openclaw-deploy-guide",
      "destination": "/openclaw-deploy-guide/index.html"
    },
    {
      "source": "/openclaw-telegram-discord",
      "destination": "/openclaw-telegram-discord/index.html"
    },
    {
      "source": "/solutions",
      "destination": "/solutions/index.html"
    },
    {
      "source": "/solutions/:slug",
      "destination": "/solutions/:slug/index.html"
    },
    {
      "source": "/vs-self-hosting",
      "destination": "/vs-self-hosting/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
